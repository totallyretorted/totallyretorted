<h1>Listing votes</h1>

<table>
  <tr>
  </tr>

<% for vote in @votes %>
  <tr>
    <td><%= link_to 'Show', retort_vote_path(@retort, vote) %></td>
    <td><%= link_to 'Edit', edit_retort_vote_path(@retort, vote) %></td>
    <td><%= link_to 'Destroy', retort_vote_path(@retort, vote), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New vote', new_retort_vote_path(@retort) %>
